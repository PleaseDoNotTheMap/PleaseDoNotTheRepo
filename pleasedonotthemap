server {
    server_name pleasedonotthemap.space www.pleasedonotthemap.space;

    root /var/www/pleasedonotthemap;
    index index.html;

    location / {
        try_files $uri $uri/ =404;
    }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/pleasedonotthemap.space/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/pleasedonotthemap.space/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    if ($host = pleasedonotthemap.space) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80;
    server_name pleasedonotthemap.space www.pleasedonotthemap.space;
    return 404; # managed by Certbot


}